{
  "name": "faircop",
  "dependencies": {
    "@remix-run/express": "^1.2.1",
    "@remix-run/react": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.1",
    "storyblok-js-client": "^4.2.1"
  },
  "devDependencies": {
    "@layer0/cli": "^4.10.2",
    "@layer0/core": "^4.10.2",
    "@layer0/devtools": "^4.10.2",
    "@layer0/express": "^4.10.2",
    "@layer0/prefetch": "^4.10.2",
    "@layer0/react": "^4.10.2",
    "@remix-run/dev": "^1.2.1",
    "@vercel/nft": "^0.17.5",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.0.23",
    "remark": "^12.0.1",
    "remark-html": "^13.0.1",
    "remark-prism": "^1.1.14",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development remix build && run-p dev:*\"",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "format": "prettier --write '**/*' --ignore-unknown",
    "start:dev": "cross-env NODE_ENV=development node server/index.js",
    "layer0:build": "npm run build && node compileSW.js && node computeNodeModules.js && layer0 build",
    "layer0:deploy": "npm run layer0:build && layer0 deploy"
  }
}
